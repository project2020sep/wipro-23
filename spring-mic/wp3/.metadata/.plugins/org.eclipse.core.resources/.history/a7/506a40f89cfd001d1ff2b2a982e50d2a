package com.demo.webflux.dto;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class Customer {

  
	private int id;
    private String name;
    
    public Customer(int i, String name) {
  		this.id = i;
  		this.name = name;
  	}
}
